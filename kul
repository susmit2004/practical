<% 
 
String ns= request.getParameter("principle"); 
String ns1= request.getParameter("year"); 
String ns2= request.getParameter("interest"); 
int n1=Integer.parseInt(ns); int 
n2=Integer.parseInt(ns1); 
float n3 = Float.valueOf(ns2); 
 
double si=((n1*n2*n3)/100); double 
x; 
x=n1+si; 
 
double r = (n3)/(12*100); 
 
int mon; mon=((n2)*12); double emi= 
(n1*r*Math.pow(1+r,mon))/(Math.pow(1+r,mon)-1); 
 
 
 
%> 
<% 
