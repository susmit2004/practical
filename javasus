Operation_atSusmit op=(Operation_atSusmit)context.getBean("opBean"); 
System.out.println("calling validate...."); 
try{ 
op.validate(85); 
}catch(Exception e){System.out.println(e);} 
System.out.println("calling validate again...."); 
try{ 
op.validate(25); 
}catch(Exception e){System.out.println(e);} 
