SusmitBean.java 
package com.example.demo; 
public class SusmitBean{ 
private String message; 
public SusmitBean(String message){ 
this.message=message; 
} 
public String getMessage(){ 
return message; 
} 
public void setMessage(String message){ 
this.message=message; 
} 
@Override 
public String toString(){ 
return String.format("SusmitBean[message=%s]",message); 
} 
} 
SusmitController.java 
package com.example.demo; 
