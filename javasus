public String helloSusmit(){ 
return "Susmit Naik is here!"; 
} 
@GetMapping(path="/hello-Susmit-bean") public 
SusmitBean helloSusmitBean(){ return new 
SusmitBean("Kaise ho Susmit?"); 
} 
} 
